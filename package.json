{
  "name": "flowmatik-components",
  "version": "0.0.1",
  "description": "Flowmatik UI components library",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write '{lib}/**/*.{js,css,json}'",
    "lint": "eslint '{lib}/**/*.js' --fix"
